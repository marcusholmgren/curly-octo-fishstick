{
  "realm": "contacts",
  "enabled": true,
  "displayName": "Contacts Realm",
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "emailVerified": true,
      "email": "testuser@example.com",
      "firstName": "Test",
      "lastName": "User",
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "testpassword",
          "temporary": false
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "contacts-app-client",
      "name": "Contacts App Client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "frontend-secret",
      "redirectUris": ["http://localhost:3000/*", "http://localhost:5173/*"],
      "webOrigins": ["http://localhost:3000", "http://localhost:5173"],
      "protocol": "openid-connect",
      "publicClient": false,
      "protocolMappers": [
        {
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "id.token.claim": "false",
            "access.token.claim": "true",
            "included.client.audience": "contacts-api-client"
          }
        },
        {
          "name": "subject-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-subject-mapper",
          "consentRequired": false,
          "config": {
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ],
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "defaultClientScopes": ["web-origins", "profile", "email", "roles"]
    },
    {
      "clientId": "contacts-api-client",
      "name": "Contacts API Client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "your-backend-client-secret",
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "defaultClientScopes": ["roles"]
    }
  ]
}
